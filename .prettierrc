/*
Prettier配置文件,代码格式化规则
*/
{
    "trailingComma": "none",
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "endOfLine": "lf",
    "printWidth": 120,
    "tabWidth": 4,
    "proseWrap": "never",
    "bracketSpacing": false,
    "arrowParens": "always",
    "overrides": [
        {
            "files": ".prettierrc",
            "options": {
                "parser": "json"
            }
        }
    ]
}
